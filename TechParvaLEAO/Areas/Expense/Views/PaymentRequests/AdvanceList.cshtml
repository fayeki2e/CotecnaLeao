@model IEnumerable<TechParvaLEAO.Areas.Expense.Models.PaymentRequest>

<link href="~/Content/PagedList.css" rel="stylesheet" type="text/css" />

@{
    ViewBag.Title = "Advance List";
}
<div class="container-fluid">
    <div class="animated fadeIn">
        <partial name="_PrimaryInfo" />
        <div class="row">
            <div class="col-lg-12">
                <nav>
                    <div class="nav nav-tabs nav-fill" id="nav-tab" role="tablist">
                        <a class="nav-item nav-link active" id="nav-home-tab" data-toggle="tab" href="#nav-home" role="tab"
                           aria-controls="nav-home" aria-selected="true">All Advance Request</a>
                        <a class="nav-item nav-link" id="nav-profile-tab" href="/Expense/PaymentRequests/@ViewData["LinkURL"]" role="tab"
                           aria-controls="nav-profile" aria-selected="false">All Expense Request</a>
                    </div>
                </nav>
                <div class="tab-content" id="nav-tabContent">
                    <div class="tab-pane fade show active" id="nav-home" role="tabpanel" aria-labelledby="nav-home-tab">
                        <partial name="_Advance" />
                    </div>
                    <div class="tab-pane fade" id="nav-profile" role="tabpanel" aria-labelledby="nav-profile-tab">

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@section Scripts {
    <script src="~/js/common.js"></script>
}
