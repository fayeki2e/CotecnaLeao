@model TechParvaLEAO.Areas.Expense.Models.ViewModels.SupportingReceivedViewModel
@{
    ViewBag.Title = "SupportingReceived";
}
<div>
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-horizontal">
            <h4>Suporting Received date will be today's date.</h4>
            <br />
            <h5>Are you sure you want to confirm?</h5>
            @Html.ValidationSummary(true, "", new { @class = "text-danger" })
            @Html.HiddenFor(model => model.PaymentRequestId)
            
        </div>
        <div class="form-actions no-color">
            <input type="submit" value="Confirm" class="btn btn-default" /> 
        </div>
    }
</div>
